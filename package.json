{
  "name": "vue2-webpack4-boilerplate",
  "version": "0.1.0",
  "description": "A Vue2 and Webpack4 boilerplate.",
  "scripts": {
    "start": "npm run dev",
    "build": "webpack --mode production ./app/index.js --config build/webpack.config.prod.js",
    "dev": "webpack-dev-server --mode development ./app/index.js --config build/webpack.config.dev.js",
    "watch": "webpack --mode production ./app/index.js --config build/webpack.config.prod.js --watch"
  },
  "dependencies": {
    "config": "^1.30.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel": "^6.23.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.4",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "comments": false
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
